{
    "template": ["configs/templates/experiment.json", "configs/templates/experiment_env.json"],
    "learner_class": "rl_algorithms.tqc.TQC",
    "log_path": "./results/panda_stack",
    "log": true,
    "log_model_interval": false,
    "learner_kwargs": {
        "gradient_steps": -1,
        "buffer_size": 1500000,
        "batch_size": 2048,
        "train_freq": [1, "episode"],
        "learning_starts": 10000,
        "learning_rate": 0.001,
        "target_update_interval": 1,
        "gamma": 0.95,
        "tau": 0.05,
        "ent_coef": "auto",
        "device": "cuda",
        "policy_kwargs": {
            "net_arch": {"qf": [512, 512, 512], "pi": [512, 512, 512]},
            "n_critics": 2
        }
    }
}