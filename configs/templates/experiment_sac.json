{
    "template": ["configs/templates/experiment.json", "configs/templates/experiment_env.json"],
    "learner_class": "rl_algorithms.sac.SAC",
    "log_path": "./results/",
    "log": true,
    "log_model_interval": false,
    "learner_kwargs": {
        "gradient_steps": 1000,
        "buffer_size": 2000000,
        "batch_size": 1024,
        "train_freq": [1, "episode"],
        "learning_starts": 10000,
        "learning_rate": 0.0001,
        "gamma": 0.99,
        "tau": 0.005,
        "ent_coef": "auto_0.1",
        "target_update_interval": 2,
        "device": "cuda",
        "policy_kwargs": {
            "net_arch": {"qf": [1024, 1024], "pi": [1024, 1024]},
            "n_critics": 2
        },
        "scheduler_kwargs": {
            "tau": 10
        }
    }
}